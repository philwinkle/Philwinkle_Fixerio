<?xml version="1.0"?>
<config>
    <modules>
        <Philwinkle_Fixerio>
             <version>0.4.1</version>
        </Philwinkle_Fixerio>
    </modules>
    <global>
        <currency>
            <import>
                <services>
                    <fixerio>
                        <name>Fixer.io</name>
                        <model>fixerio/import</model>
                    </fixerio>
                </services>
            </import>
        </currency>
        <models>
            <fixerio>
                <class>Philwinkle_Fixerio_Model</class>
            </fixerio>
        </models>
        <helpers>
            <fixerio>
                <class>Philwinkle_Fixerio_Helper</class>
            </fixerio>
        </helpers>
    </global>
    <default>
        <currency>
            <fixerio>
                <timeout>100</timeout>
                <api_key backend_model="adminhtml/system_config_backend_encrypted"/>
                <active>1</active>
            </fixerio>
        </currency>
    </default>
</config>
